<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="apigeeManagerImpl" class="manager.ApigeeManagerImpl">
		<property name="apiModel" ref="apiModel" />
	</bean>
	<bean id="apiModel" class="model.ApiModel">
		<property name="question" value="Are you using Apigee API Management ?" />
	</bean>

	<bean id="masheryManagerImpl" class="manager.MasheryManagerImpl"></bean>

	<bean id="apiManagementService" class="service.ApiManagementService">
		<property name="apiManager" ref="apigeeManagerImpl" />
	</bean>

</beans>